{ 
  "manifest_version": 2,
   "name": "FullShot",
   "description": "Take a full screenshot of your wepage",
    "version": "1.0",
     "author": "Bami Ogunfemi",
     "background": {
      "scripts": ["content.js"],
      "persistent": false
    },
    "page_action": {
      "default_popup": "popup.html"
    },
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "permissions": ["storage"]
     
}
